import re

def abstract(svg):
    rmsvg = re.replace('<\w>',svg)
    return rmsvg.group(0)

def deabstract():
    return 0

test = '''<svg xmlns="http://www.w3.org/2000/svg" version="1.1">
  <rect x="50" y="20" width="150" height="150"
  style="fill:blue;stroke:pink;stroke-width:5;fill-opacity:0.1;
  stroke-opacity:0.9"/>
</svg>'''

print abstract(test)
